{
  "version": 2,
  "builds": [
    {
      "src": "client/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "server/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "/server/api/index"
    },
    {
      "source": "/:path*",
      "destination": "/client/:path*"
    }
  ]
}
